	</head>


	<body>
		<nav class="navbar navbar-light" style="height: 100px">
			<a class="navbar-brand" href={{path('post_index') }}>
				<img src="{{asset('uploads/post_images/logo_last_chance.png')}}" alt="chaise" height="80">
				<p class="pl-2 d-inline font-weight-bold" style="color: black;">
					<strong>Last Chance</strong>
				</p>
			</a>


			{% if app.user %}
				<a class="navbar-brand" href={{path('post_new') }}>
					<p class="pl-2 d-inline font-weight-bold;">
						<strong>Créer un post</strong>
					</p>
				</a>
				<a class="navbar-brand" href="{{path('app_logout')}}">
					<p class="pl-2 d-inline font-weight-bold;"></p>
					<strong>Se déconnecter</strong>
				</a>

			{% else %}

				<a class="navbar-brand" href={{path('app_register')}}>

					<p class="pl-2 d-inline font-weight-bold;">
						<strong>S'inscrire</strong>

					</p>
				</a>
				<a class="navbar-brand" href="{{path('app_login')}}">

					<p class="pl-2 d-inline font-weight-bold;"></p>
					<strong>Se connecter</strong>

				</a>
			{% endif %}
		</body>
	</body>
